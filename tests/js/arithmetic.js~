$('summation').innerText = '$math.sum([1,2,3,4,5]) = ' + $math.sum([1,2,3,4,5]);
$('summation-1').innerText = '$math.sum([624, 231, 2321, 2312, 646347, 7233, 62532]) = ' + $math.sum([624, 231, 2321, 2312, 646347, 7233, 62532]);

$('diff').innerText = '$math.diff([10, 2, 4]) = ' + $math.diff([10, 2, 4]);
$('diff-1').innerText = '$math.diff(743, 1255) = ' + $math.diff(743, 1255);

$('product').innerText = '$math.product([1,2,4,5]) = ' + $math.product([1,2,4,5]);
$('product-1').innerText = '$math.product(534, 742) = ' + $math.product(534, 742);

$('quotient').innerText = '$math.quotient([42, 6, 4]) = ' + $math.quotient([42, 6, 4]);
$('quotient-1').innerText = '$math.quotient(951, 23) = ' + $math.quotient(951, 23);
